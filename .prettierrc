{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/screens/(.*)$",
		"^@/components/(.*)$",
		"^@/hooks/(.*)#",
		"^@/services/(.*)$",
		"^@/navigation/(.*)$",
		"^@/assets/(.*)$",
		"^@/providers/(.*)$",
		"^@/constants/(.*)$",
		"^@/regex/(.*)$",
		"^@/types/(.*)$",
		"^../(.*)$",
		"^./(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
