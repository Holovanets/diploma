{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			// "@/*": ["./app/*"],
			"@/assets": ["./app/assets"],
			"@/assets/*": ["./app/assets/*"],
			"@/components": ["./app/components"],
			"@/components/*": ["./app/components/*"],
			"@/constants": ["./app/constants"],
			"@/constants/*": ["./app/constants/*"],
			"@/hooks": ["./app/hooks"],
			"@/hooks/*": ["./app/hooks/*"],
			"@/modules": ["./app/modules"],
			"@/modules/*": ["./app/modules/*"],
			"@/navigation": ["./app/navigation"],
			"@/navigation/*": ["./app/navigation/*"],
			"@/providers": ["./app/providers"],
			"@/providers/*": ["./app/providers/*"],
			"@/regex": ["./app/regex"],
			"@/regex/*": ["./app/regex/*"],
			"@/screens": ["./app/screens"],
			"@/screens/*": ["./app/screens/*"],
			"@/types": ["./app/types"],
			"@/reducers/*": ["./app/reducers/*"],
			"@/reducers": ["./app/reducers"],
			"@/actions/*": ["./app/actions/*"],
			"@/actions": ["./app/actions"],
			"@/types/*": ["./app/types/*"],
			"@/utils": ["./app/utils"],
			"@/utils/*": ["./app/utils/*"],
			"@/services/*": ["./app/services/*"],
			"@/services": ["./app/services"]
		}
	}
}
